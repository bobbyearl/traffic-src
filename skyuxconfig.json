{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@blackbaud/skyux-builder-plugin-stache"
  ],
  "app": {
    "title": "Bobby Earl Traffic",
    "styles": [
      "src/app/styles.scss"
    ]
  },
  "appSettings": {
    "bffServiceUrl": "https://bobbyearl-traffic.herokuapp.com",
    "stache": {
      "googleAnalytics": {
        "clientId": "UA-460794-1"
      }
    }
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
